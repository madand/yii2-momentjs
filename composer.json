{
    "name": "madand/yii2-momentjs",
    "description": "moment asset for Yii2",
    "keywords": [
        "yii",
        "yii2",
        "moment"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/MadAnd/yii2-momentjs",
    "authors": [
        {
            "name": "Andriy Kmit'",
            "email": "dev@madand.net",
            "homepage": "https://github.com/MadAnd",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/moment": "*"
    },
    "autoload": {
        "psr-4": {
            "madand\\momentjs\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
